<main class="container posts">
    <ul>
        <% site.posts.each(function(post, i){ %>
          <!-- <% console.log(Object.keys(post)) %> -->
          <li> <a href="<%- config.root + post.path %>"><%- post.title %></a>
            <span class="post-item-date"><%- date(post.date,'M月D日') %></span>
          </li>
        <% }) %>
    </ul> 
</main>